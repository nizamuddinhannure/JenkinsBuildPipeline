pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building Application'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing Application'
            }
        }
        
        stage('Release') {
            steps {
                echo 'Releasing Application'
            }
        }
        
    }
}
